name="typescript"
aliases=("ts" "typescript")
requires=("node")

version(){
	tsc --version | grep -Po '\d+.\d+.\d+'
}
install(){
	npm i -g typescript
}
pre_uninstall(){}
post_uninstall(){}
path(){} # Built into node
compile(){
	mv code.code interim.ts
	tsc interim.ts
	rm -f interim.ts
	mv interim.js code.code
}
run(){
	xargs -a args.args -d '\n' node code.code < stdin.stdin
}