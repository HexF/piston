name="csharp"
aliases=("c#" "cs" "csharp")
requires=()

version(){
	mcs --version | grep -Po '\d+.\d+.\d+'
}
install(){
	apt install -y mono-complete
}
pre_uninstall(){}
post_uninstall(){}
path(){}
compile(){
	mcs $(echo code.code | sed 's/\///') -nowarn:0219 -out:binary
}
run(){
	xargs -a args.args -d '\n' mono binary < stdin.stdin
}