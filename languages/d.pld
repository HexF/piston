name="d"
aliases=("dlang" "d")
requires=()

version(){
	dmd --version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget http://downloads.dlang.org/releases/2.x/2.095.0/dmd.2.095.0.linux.tar.xz
	unxz dmd.2.095.0.linux.tar.xz
	tar -xf dmd.2.095.0.linux.tar
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/dmd2/linux/bin64"
}
compile(){
	cp code.code code.d
	dmd code.d
}
run(){
	xargs -a args.args -d '\n' ./code
}