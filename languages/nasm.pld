name="nasm"
aliases=("asm" "nasm")
requires=()

version(){
	nasm -version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://www.nasm.us/pub/nasm/releasebuilds/2.14.02/nasm-2.14.02.tar.gz
	tar -xzf nasm-2.14.02.tar.gz
	cd nasm-2.14.02
	./configure
	make
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/nasm-2.14.02"
}
compile(){
	nasm -f elf32 -o binary.o code.code
	ld -m elf_i386 binary.o -o binary
}
run(){
	xargs -a args.args -d '\n' ./binary < stdin.stdin
}