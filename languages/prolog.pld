name="prolog"
aliases=("prolog" "plg")
requires=()

version(){
	swipl --version | grep -Po '\d+.\d+.\d+'
}
install(){}
pre_uninstall(){}
post_uninstall(){}
path(){}
compile(){}
run(){
	sed 's/^.*$/:- forall((Goal = (\0), call(Goal)), (write(Goal), nl))./' input.input |
		cat code.code - > code.pl
	
	timeout -s KILL 3 swipl -g true -t halt code.pl
}