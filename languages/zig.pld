name="zig"
aliases=("zig")
requires=()

version(){
	zig version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://ziglang.org/download/0.7.1/zig-linux-x86_64-0.7.1.tar.xz
	tar -xf zig-linux-x86_64-0.7.1.tar.xz
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/zig-linux-x86_64-0.7.1"
}
compile(){
	cp code.code main.zig
	zig build-exe main.zig
}
run(){
	xargs -a args.args -d '\n' ./main
}