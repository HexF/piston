name="nim"
aliases=("nim")
requires=()

version(){
	nim -v | grep -Po '\d+.\d+.\d+'
}
install(){
	unxz nim-1.4.0-linux_x64.tar.xz
	tar -xf nim-1.4.0-linux_x64.tar
	cd nim-1.4.0
	./install.sh $LBIN
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/nim/bin"
}
compile(){
	nim --hints:off c code.code
}
run(){
	xargs -a args.args -d '\n' ./code
}