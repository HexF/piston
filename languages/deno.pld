name="deno"
aliases=("deno" "denojs" "denots")
requires=()

version(){
	deno --version | grep -Po '\d+.\d+.\d+'
}
install(){
	# TODO: Move this into $LBIN
	curl -fsSL https://deno.land/x/install/install.sh | sh
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$HOME/.deno/bin"
}
compile(){}
run(){
	xargs -a args.args -d '\n' deno run code.code < stdin.stdin
}