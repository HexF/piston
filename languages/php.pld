name="php"
aliases=("php" "php3" "php4" "php5")
requires=()

version(){
	php -v | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://www.php.net/distributions/php-8.0.0.tar.gz
	tar -xzf php-8.0.0.tar.gz
	cd php-8.0.0
	./configure
	make
	make install
	#TODO: Installs globally (can we change this?)
}
pre_uninstall(){}
post_uninstall(){}
path(){}
compile(){}
run(){
	xargs -a args.args -d '\n' php code.code < stdin.stdin
}