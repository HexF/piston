name="cpp"
aliases=("c++" "cpp" "cc" "cxx")
requires=()

version(){
	g++ --version | grep -Po '\d+.\d+.\d+'
}
install(){
	apt install -y g++
}
pre_uninstall(){}
post_uninstall(){}
path(){}
compile(){
	g++ -std=c++17 -o binary -x c++ code.code
}
run(){
	timeout -s KILL 3 xargs -a args.args -d '\n' ./binary < stdin.stdin
}