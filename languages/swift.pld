name="swift"
aliases=("swift")
requires=()

version(){
	swift --version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://swift.org/builds/swift-5.1.5-release/ubuntu1804/swift-5.1.5-RELEASE/swift-5.1.5-RELEASE-ubuntu18.04.tar.gz
	tar -xzf swift-5.1.5-RELEASE-ubuntu18.04.tar.gz
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/swift-5.1.5-RELEASE-ubuntu18.04/usr/bin"
}
compile(){}
run(){
	xargs -a args.args -d '\n' swift code.code < stdin.stdin
}