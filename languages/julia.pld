name="julia"
aliases=("jl" "julia")
requires=()

version(){
	julia --version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://julialang-s3.julialang.org/bin/linux/x64/1.5/julia-1.5.0-linux-x86_64.tar.gz
	tar -xzf julia-1.5.0-linux-x86_64.tar.gz
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/julia-1.5.0/bin"
}
compile(){}
run(){
	xargs -a args.args -d '\n' julia code.code < stdin.stdin
}