name="node"
aliases=("javascript" "js" "node" "node.js")
requires=()

version(){
	node --version | grep -Po '\d+.\d+.\d+'
}
install(){
	wget https://nodejs.org/dist/v12.16.1/node-v12.16.1-linux-x64.tar.xz
	unxz node-v12.16.1-linux-x64.tar.xz
	tar -xf node-v12.16.1-linux-x64.tar
}
pre_uninstall(){}
post_uninstall(){}
path(){
	echo "$LBIN/node-v12.16.1-linux-x64/bin"
}
compile(){}
run(){
	xargs -a args.args -d '\n' node code.code < stdin.stdin
}