name="haskell"
aliases=("haskell" "hs")
requires=()

version(){
	ghc --version | grep -Po '\d+.\d+.\d+'
}
install(){
	# TODO: Installs globally
	apt install -y ghc
}
pre_uninstall(){}
post_uninstall(){}
path(){}
compile(){
	cp code.code code.hs
	ghc -dynamic -o binary code.hs > /dev/null 2>&1
}
run(){
	xargs -a args.args -d '\n' ./binary < stdin.stdin
}